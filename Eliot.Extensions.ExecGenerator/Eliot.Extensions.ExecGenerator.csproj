    <OutputPath>bin\DecompileDebug\</OutputPath>
    <CodeAnalysisLogFile>..\..\..\..\..\..\..\Program Files (x86)\UE Explorer\Extensions\Eliot.Extensions.ExecGenerator.dll.CodeAnalysisLog.xml</CodeAnalysisLogFile>
